<% include partials/_headers %>
<div class="container my-5">
        <div class="col-md-12">
            <div class="card-body" style="background-color: transparent;" >
                <div style="display: inline-block;">
                    <button type="submit" id="adm1"
                            onclick="location.href='/signin'">Ingresar Bicicleta
                    </button>
                </div>
                <div  style="display: inline-block;">
                    <button type="submit" id="adm2"
                            onclick="location.href='/alquilar'">Registrar Estación
                    </button>
                </div>
            </div>
        </div>
</div>
<div class="container my-9">
    <div class="col-md-12">
        <div class="card" style="background-color:#2C3E50; font-weight: bold; color:#D3D3D3; margin-top:50px">
            <div class="card-header">
                <h3 >PUCMM</h3>
            </div>
            <div class="card-body" style="background-color:#DCDCDC;">
                <script src="https://cdn.rawgit.com/openlayers/openlayers.github.io/master/en/v5.3.0/build/ol.js"></script>
                <div id="map" class="map"></div>
                <script>
                    var map = new ol.Map({
                        layers: [new ol.layer.Tile({source: new ol.source.OSM()})],
                        target: 'map',
                        view: new ol.View({
                            projection: 'EPSG:4326',
                            center: [-70.6827,19.4436],
                            zoom: 16})});
                </script>
            </div>
        </div>
    </div>
</div>
